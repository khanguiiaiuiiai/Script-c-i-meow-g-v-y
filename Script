local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local VirtualUser = game:GetService("VirtualUser")

-- Settings
_G.Settings = { WalkSpeed = 50, JumpPower = 80 }

-- GUI
local gui = Instance.new("ScreenGui")
gui.Name = "MeowGUI"
gui.Parent = LocalPlayer:WaitForChild("PlayerGui")

local main = Instance.new("Frame")
main.Size = UDim2.new(0, 380, 0, 550)
main.Position = UDim2.new(0.7, 0, 0.2, 0)
main.BackgroundColor3 = Color3.fromRGB(30,30,40)
main.Draggable = true
main.Active = true
main.Parent = gui
Instance.new("UICorner", main).CornerRadius = UDim.new(0,12)

local title = Instance.new("TextLabel", main)
title.Size = UDim2.new(1,0,0,45)
title.BackgroundColor3 = Color3.fromRGB(50,50,70)
title.Text = "üê± C√ÅI MEOW G√å V·∫¨Y"
title.TextColor3 = Color3.new(1,1,1)
title.Font = Enum.Font.GothamBold
title.TextSize = 22
Instance.new("UICorner", title).CornerRadius = UDim.new(0,12)

local scroll = Instance.new("ScrollingFrame", main)
scroll.Size = UDim2.new(1,-20,1,-95)
scroll.Position = UDim2.new(0,10,0,55)
scroll.BackgroundTransparency = 1
scroll.CanvasSize = UDim2.new(0,0,0,800)

local layout = Instance.new("UIGridLayout", scroll)
layout.CellSize = UDim2.new(0,170,0,50)
layout.CellPadding = UDim2.new(0,8,0,8)

local function btn(name, color, cb)
    local b = Instance.new("TextButton", scroll)
    b.Size = UDim2.new(0,170,0,50)
    b.BackgroundColor3 = color
    b.Text = name
    b.TextColor3 = Color3.new(1,1,1)
    b.Font = Enum.Font.GothamSemibold
    b.TextSize = 15
    Instance.new("UICorner", b).CornerRadius = UDim.new(0,8)
    b.MouseButton1Click:Connect(cb)
    return b
end

-- TELEPORT ƒê·∫¢O
local islands = {
    {"Sea1: Start", CFrame.new(-72,18,1278)},
    {"Sea1: Jungle", CFrame.new(-1633,13,33)},
    {"Sea1: Pirate", CFrame.new(-1186,5,830)},
    {"Sea1: Desert", CFrame.new(952,16,1445)},
    {"Sea1: Snow", CFrame.new(1186,27,-1393)},
    {"Sea2: Mansion", CFrame.new(-12475,334,-7455)},
    {"Sea2: Factory", CFrame.new(-4246,23,-4342)},
    {"Sea2: Green", CFrame.new(-5537,24,-6575)},
    {"Sea3: Port", CFrame.new(-296,27,-28354)},
    {"Sea3: Hydra", CFrame.new(5637,610,-5466)},
    {"Sea3: Great Tree", CFrame.new(2145,514,-4627)},
}
for _, v in ipairs(islands) do
    btn(v[1], Color3.fromRGB(80,120,200), function()
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            char.HumanoidRootPart.CFrame = v[2]
        end
    end)
end

-- Speed
btn("‚ûï TƒÉng Speed", Color3.fromRGB(100,200,100), function()
    _G.Settings.WalkSpeed = _G.Settings.WalkSpeed + 10
    if LocalPlayer.Character then
        LocalPlayer.Character.Humanoid.WalkSpeed = _G.Settings.WalkSpeed
    end
end)
btn("‚ûñ Gi·∫£m Speed", Color3.fromRGB(200,100,100), function()
    _G.Settings.WalkSpeed = math.max(16, _G.Settings.WalkSpeed - 10)
    if LocalPlayer.Character then
        LocalPlayer.Character.Humanoid.WalkSpeed = _G.Settings.WalkSpeed
    end
end)

-- TikTok
btn("üì± TikTok @tddb9812", Color3.fromRGB(255,0,100), function()
    print("‚úÖ Theo d√µi TikTok @tddb9812 nha!")
end)

